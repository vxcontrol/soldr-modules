{
    "name": "file_reader",
    "template": "empty",
    "version": {
        "major": 1,
        "minor": 0,
        "patch": 0
    },
    "os": {
        "linux": [
            "amd64"
        ],
        "windows": [
            "amd64"
        ]
    },
    "system": false,
    "actions": [
        "frd_rewind_logfile"
